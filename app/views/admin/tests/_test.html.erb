<tr>
  <td>
    <%= test_counter + 1 %>
  </td>
  <td>
    <%= test.title %>
  </td>
  <td>
    <%= link_to test.questions.count, admin_test_questions_path(test_id: test) %>
  </td>
  <td>
    <%= link_to 'Show', admin_test_path(test) %>
  </td>
  <td>
    <%= link_to 'Edit', edit_admin_test_path(test) %>
  </td>
  <td>
    <%= button_to "Delete", admin_test_path(test), method: :delete, data: {confirm: "Вы уверены?"} %>
  </td>
</tr>
